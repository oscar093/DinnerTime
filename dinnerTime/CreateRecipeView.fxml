            <TextArea fx:id="taInstruction" layoutX="346.0" layoutY="35.0" prefHeight="449.0" prefWidth="288.0" />
            <Label layoutX="346.0" layoutY="16.0" text="Instruktioner:" />
            <ComboBox fx:id="cbCountry" layoutX="139.0" layoutY="99.0" prefHeight="25.0" prefWidth="196.0" />
            <Button fx:id="btnPicture" layoutX="242.0" layoutY="237.0" mnemonicParsing="false" onAction="#addPicture" text="Lägg till bild" />
            <TextField fx:id="tfPicture" layoutX="139.0" layoutY="210.0" prefHeight="27.0" prefWidth="196.0" />
            <Label layoutX="16.0" layoutY="215.0" text="Sökväg till bild:" />
