<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane fx:id="pane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dinnerTime.CreateRecipeController">
   <children>
      <Pane layoutY="-1.0" prefHeight="538.0" prefWidth="651.0">
         <children>
            <Button fx:id="btnSend" layoutX="315.0" layoutY="499.0" mnemonicParsing="false" onAction="#sendRecipe" text="Skicka!" />
            <Label layoutX="11.0" layoutY="24.0" prefHeight="17.0" prefWidth="60.0" text="Rätt:" />
            <Label layoutX="14.0" layoutY="54.0" />
            <Label layoutX="11.0" layoutY="62.0" text="Tillagningstid: " />
            <Label layoutX="8.0" layoutY="103.0" />
            <Label layoutX="12.0" layoutY="103.0" text="Land: " />
            <TextField fx:id="tfTitle" layoutX="137.0" layoutY="19.0" prefHeight="27.0" prefWidth="196.0" />
            <TextField fx:id="tfTime" layoutX="138.0" layoutY="57.0" prefHeight="27.0" prefWidth="196.0" />
            <TextArea fx:id="taIngredients" editable="false" layoutX="16.0" layoutY="305.0" prefHeight="177.0" prefWidth="317.0" />
            <Label layoutX="14.0" layoutY="144.0" text="Ingrediens:" />
            <TextField fx:id="tfIngredientInput" layoutX="140.0" layoutY="139.0" prefHeight="27.0" prefWidth="193.0" />
