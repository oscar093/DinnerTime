<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane fx:id="pane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dinnerTime.CreateRecipeController">
   <children>
      <Pane prefHeight="407.0" prefWidth="355.0">
         <children>
            <Button fx:id="btnSend" layoutX="149.0" layoutY="368.0" mnemonicParsing="false" onAction="#sendRecipe" text="Skicka!" />
            <Label layoutX="11.0" layoutY="24.0" prefHeight="17.0" prefWidth="60.0" text="Rätt:" />
            <Label layoutX="14.0" layoutY="54.0" />
            <Label layoutX="11.0" layoutY="62.0" text="Tillagningstid: " />
            <Label layoutX="8.0" layoutY="103.0" />
            <Label layoutX="12.0" layoutY="103.0" text="Land: " />
            <TextField fx:id="tfTitle" layoutX="137.0" layoutY="19.0" prefHeight="27.0" prefWidth="196.0" />
            <TextField fx:id="tfTime" layoutX="138.0" layoutY="57.0" prefHeight="27.0" prefWidth="196.0" />
            <TextField fx:id="tfCountry" layoutX="140.0" layoutY="97.0" prefHeight="27.0" prefWidth="194.0" />
            <TextArea fx:id="taIngredients" disable="true" editable="false" layoutX="15.0" layoutY="217.0" prefHeight="134.0" prefWidth="317.0" />
